{
  "name": "@elizaos/eliza-starter",
  "version": "0.1.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "clean": "./scripts/clean.sh",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all",
    "migration": "sequelize-cli db:migrate --config src/config/config.json --migrations-path src/migrations --models-path src/models",
    "swagger-autogen": "node src/api/swagger.js"
  },
  "dependencies": {
    "@elizaos/adapter-postgres": "0.1.8",
    "@elizaos/adapter-sqlite": "0.1.8",
    "@elizaos/client-auto": "0.1.8",
    "@elizaos/client-discord": "0.1.8",
    "@elizaos/client-telegram": "0.1.8",
    "@elizaos/client-twitter": "0.1.8",
    "@elizaos/core": "0.1.8",
    "@elizaos/plugin-bootstrap": "0.1.8",
    "@elizaos/plugin-image-generation": "0.1.8",
    "@elizaos/plugin-node": "0.1.8",
    "@elizaos/plugin-solana": "0.1.8",
    "@elizaos/plugin-starknet": "0.1.8",
    "@tavily/core": "0.0.2",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/express-serve-static-core": "^5.0.6",
    "amqplib": "0.10.5",
    "body-parser": "1.20.3",
    "cors": "2.8.5",
    "discord.js": "14.16.3",
    "express": "4.21.1",
    "express-basic-auth": "^1.2.1",
    "multer": "1.4.5-lts.1",
    "net": "1.0.2",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "readline": "1.3.0",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "ws": "8.18.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0"
    }
  },
  "devDependencies": {
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.10",
    "@types/sequelize": "^4.28.20",
    "sequelize-cli": "^6.6.2",
    "swagger-autogen": "^2.23.7",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  }
}
